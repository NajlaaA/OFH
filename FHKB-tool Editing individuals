    /**
     * @param args the command line arguments
     */
    public void setModifyValues(String ontID,String preIndLabel,String moFirst,String moLast, String moBirth, String moDeath, String Emother,String Efather, DefaultListModel m , int cls)
    {
      
        
      moLabel=moFirst+"_"+moLast+"_"+moBirth;
      model=m;
      
      //Load ontolgy again
     if(ontID.equals("min"))
     { try{
            manager = OWLManager.createOWLOntologyManager();
            File file1 = new File("C://Users/najla/Desktop/MSc project/OWL API/fhkb-minimumclasses-noabox.owl");
            ont = manager.loadOntologyFromOntologyDocument(file1);
           
          }catch(OWLOntologyCreationException e )
          {
          System.err.println("OWLOntologyCreationException: " + e.getMessage()); 
          }
     }
      else if(ontologyID.equals("core"))
      {
          try{
             manager = OWLManager.createOWLOntologyManager();
            File file1 = new File("C://Users/najla/Desktop/MSc project/OWL API/fhkb-coreclasses-noabox.owl");
            ont = manager.loadOntologyFromOntologyDocument(file1);
            
           
        }catch(OWLOntologyCreationException e )
        {
        System.err.println("OWLOntologyCreationException: " + e.getMessage()); 
        }
      }
      else if(ontologyID.equals("most"))
      {
          try{
             manager = OWLManager.createOWLOntologyManager();
            File file1 = new File("C://Users/najla/Desktop/MSc project/OWL API/fhkb-mostclasses-noabox.owl");
            ont = manager.loadOntologyFromOntologyDocument(file1);
            
        }catch(OWLOntologyCreationException e )
        {
        System.err.println("OWLOntologyCreationException: " + e.getMessage()); 
        }
      }
     
     //to delete previous label
     OWLNamedIndividual removedInd = df.getOWLNamedIndividual(IRI.create("http://www.owl.cs.manchester.ac.uk/ontologies/fhkb.owl#"+preIndLabel));
     OWLEntityRemover remover = new OWLEntityRemover(manager, Collections.singleton(ont));
     removedInd.accept(remover);
     manager.applyChanges(remover.getChanges());
     remover.reset();
     list1.setModel(model);
     model.removeElement(preIndLabel);
      
      //Adding the new label
      OWLIndividual ind = df.getOWLNamedIndividual(IRI.create("http://www.owl.cs.manchester.ac.uk/ontologies/fhkb.owl#"+moLabel));
      OWLClassAssertionAxiom classAssertion11;
      OWLClassAssertionAxiom ca1;
          if(cls==1)
               classAssertion11 = df.getOWLClassAssertionAxiom(Man, ind);
          else if(cls==2)
               classAssertion11= df.getOWLClassAssertionAxiom(Woman, ind);
          else
            classAssertion11= df.getOWLClassAssertionAxiom(Person, ind);
        
         
      manager.addAxiom(ont, classAssertion11);
      model.addElement(moLabel);
      list1.setModel(model);
      
      
       //Adding new data property
       OWLAxiom asser11= df.getOWLDataPropertyAssertionAxiom(hasFirstName, ind, moFirst);
          manager.addAxiom(ont,asser11);
          OWLAxiom asser22= df.getOWLDataPropertyAssertionAxiom(hasFamilyName, ind, moLast);
          manager.addAxiom(ont,asser22);
          OWLAxiom asser33= df.getOWLDataPropertyAssertionAxiom(hasBirthYear, ind, moBirth);
          manager.addAxiom(ont,asser33);
          
       //Adding father
          if(!Efather.isEmpty())
          {OWLIndividual indFather = df.getOWLNamedIndividual(IRI.create("http://www.owl.cs.manchester.ac.uk/ontologies/fhkb.owl#"+Efather));
          OWLAxiom assertion88 = df.getOWLObjectPropertyAssertionAxiom(hasFather, ind, indFather);
          manager.addAxiom(ont, assertion88);}
       //Adding mother
          if(!Emother.isEmpty())
          {OWLIndividual indMother = df.getOWLNamedIndividual(IRI.create("http://www.owl.cs.manchester.ac.uk/ontologies/fhkb.owl#"+Emother));
          OWLAxiom assertion99 = df.getOWLObjectPropertyAssertionAxiom(hasMother, ind, indMother);
          manager.addAxiom(ont, assertion99);}
            
       try {

            manager.saveOntology(ont);
        } catch (OWLOntologyStorageException ex) {
            Logger.getLogger(LoadOntology.class.getName()).log(Level.SEVERE, null, ex);
        }
    }
    
    
